<?xml version="1.0" encoding="utf-8" ?>
<skinSetting displayName="红色主题">
  <!--皮肤图片资源声明段，只有此段中声明的图片，才会被加载-->
  <imageSources>
    <!--贴图资源-->
    <image name="image_apple" fileName="apple.png"></image>
    <image name="image1" fileName="test-1.png"></image>
    <image name="image2" fileName="test-2.png"></image>
    <image name="baidu" fileName="baidu.gif"></image>
    <!--背景-->
    <image name="background-normal" fileName="background-normal.png"></image>
    <image name="background-max" fileName="background-max.png"></image>
    <image name="background-test" fileName="background-test.png"></image>
    <!--系统按钮资源-->
    <image name="RestoreButton_Normal" fileName="IMRestoreButton_Normal.bmp"></image>
    <image name="RestoreButton_Hover" fileName="IMRestoreButton_Hover.bmp"></image>
    <image name="RestoreButton_Down" fileName="IMRestoreButton_Down.bmp"></image>

    <image name="MinButton_Normal" fileName="IMMinButton_Normal.bmp"></image>
    <image name="MinButton_Hover" fileName="IMMinButton_Hover.bmp"></image>
    <image name="MinButton_Down" fileName="IMMinButton_Down.bmp"></image>

    <image name="MaxButton_Normal" fileName="IMMaxButton_Normal.bmp"></image>
    <image name="MaxButton_Hover" fileName="IMMaxButton_Hover.bmp"></image>
    <image name="MaxButton_Down" fileName="IMMaxButton_Down.bmp"></image>

    <image name="CloseButton_Normal" fileName="IMCloseButton_Normal.bmp"></image>
    <image name="CloseButton_Hover" fileName="IMCloseButton_Hover.bmp"></image>
    <image name="CloseButton_Down" fileName="IMCloseButton_Down.bmp"></image>

    <!--普通按钮资源-->
    <image name="VertivalButton_Normal" fileName="纵向普通-#080808.png"></image>
    <image name="VertivalButton_Hover" fileName="纵向触摸-#080808.png"></image>
    <image name="VertivalButton_Down" fileName="纵向选中-#ffffff.png"></image>
  </imageSources>
  <!--皮肤参数配置段，存储皮肤各种配置参数，例如配色，字体大小，线条粗细等信息-->
  <skinParameters>
    <parameter name="listview1-location-x" value="17"/>
    <parameter name="listview1-location-y" value="147"/>
    <parameter name="listview1-location-width" value="400"/>
    <parameter name="listview1-location-height" value="130"/>
    <parameter name="button1-location-x" value="17"/>
    <parameter name="button1-location-y" value="292"/>
    <parameter name="button2-location-x" value="132"/>
    <parameter name="button2-location-y" value="292"/>
  </skinParameters>
</skinSetting>